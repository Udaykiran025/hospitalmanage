<mvc:View controllerName="hospitalmanage.controller.Patientlist"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="patientlist" title="Patient List">
        <content >

             <Table id="_IDGenTable1" items="{/zpatientSet}">
                <columns>
                    <Column id="_IDGenColumn145"><Text id="_IDGenText233" text="Pid"/></Column>
                    <Column id="_IDGenColumn1"><Text id="_IDGenText3" text="Fname"/></Column>
                    <Column id="_IDGenColumn2"><Text id="_IDGenText4" text="Lname"/></Column>
                    <Column id="_IDGenColumn3"><Text id="_IDGenText5" text="Gender"/></Column>
                    <Column id="_IDGenColumn33"><Text id="_IDGenText517" text="Email"/></Column>
                    <Column id="_IDGenColumn3039"><Text id="_IDGenText581" text="Contact"/></Column>
                    <Column id="_IDGenColumn323"><Text id="_IDGenText54561" text="Password"/></Column>
                    <!--Column id="_IDGenColumn023"><Text id="_IDGenText049" text="Action"/></Column-->
                </columns>
                <items>
                    <ColumnListItem id="_IDGenColumnListItem1" type="Navigation" press="onNavObject">
                        <cells>
                            <Text id="_IDGenText566" text="{Pid}"/>
                            <Text id="_IDGenText6" text="{Fname}"/>
                            <Text id="_IDGenText7" text="{Lname}"/>
                            <Text id="_IDGenText87" text="{Gender}"/>
                            <Text id="_IDGenText21" text="{Email}"/>
                            <Text id="_IDGenText8387" text="{Contact}"/>
                            <Text id="_IDGenText2261" text="{Password}"/>
                            
                            <!--HBox>
                                <Button id="_IDGenButton31" class='sapUiTinyMarginEnd' icon="sap-icon://delete" press="ondelete"></Button>
                                <Button id="_IDGenButton1" icon="sap-icon://edit-outside" press="onupdate"></Button>
                            </HBox-->
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
				</content>

         
     
    </Page>
</mvc:View>
