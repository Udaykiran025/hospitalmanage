<mvc:View controllerName="hospitalmanage.controller.Admin"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="admin" title="Doctors List">
        <content >

             <Table id="_IDGenTable1" items="{/zdoctorSet}">
                <columns>
                    <Column id="_IDGenColumn029"><Text id="_IDGenText393" text="Doctorid"/></Column>
                    <Column id="_IDGenColumn145"><Text id="_IDGenText233" text="Username"/></Column>
                    <Column id="_IDGenColumn1"><Text id="_IDGenText3" text="Password"/></Column>
                    <Column id="_IDGenColumn2"><Text id="_IDGenText4" text="Email"/></Column>
                    <Column id="_IDGenColumn3"><Text id="_IDGenText5" text="Specialization"/></Column>
                    <Column id="_IDGenColumn33"><Text id="_IDGenText51" text="Docfees"/></Column>
                    <!--Column id="_IDGenColumn023"><Text id="_IDGenText049" text="Action"/></Column-->
                </columns>
                <items>
                    <ColumnListItem id="_IDGenColumnListItem1" type="Navigation" press="onNavObject">
                        <cells>
                            <Text id="_IDGenText376" text="{Doctorid}"/>
                            <Text id="_IDGenText566" text="{Username}"/>
                            <Text id="_IDGenText6" text="{Password}"/>
                            <Text id="_IDGenText7" text="{Email}"/>
                            <Text id="_IDGenText87" text="{Specialization}"/>
                            <Text id="_IDGenText21" text="{Docfees}"/>
                            
                            <!--HBox>
                                <Button id="_IDGenButton31" class='sapUiTinyMarginEnd' icon="sap-icon://delete" press="ondelete"></Button>
                                <Button id="_IDGenButton1" icon="sap-icon://edit-outside" press="onupdate"></Button>
                            </HBox-->
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
				</content>

         
     
    </Page>
</mvc:View>
